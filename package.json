{
  "name": "gulp-memory-fs",
  "version": "3.0.0",
  "description": "Memory file system can also be used in gulp environment. - gulp环境下也可以使用内存文件系统。",
  "author": {
    "name": "段昊辰",
    "email": "duanhaochen@126.com",
    "url": "https://github.com/duan602728596"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:duan602728596/gulp-memory-fs.git"
  },
  "homepage": "https://github.com/duan602728596/gulp-memory-fs",
  "license": "MIT",
  "keywords": [
    "gulp",
    "gulpplugin",
    "gulp-memory-fs",
    "memory-fs",
    "memfs",
    "fs",
    "memory",
    "browser",
    "koa",
    "mock",
    "proxy"
  ],
  "main": "lib/cjs.js",
  "exports": {
    ".": {
      "import": "./esm/GulpMemoryFs.js",
      "default": "./lib/cjs.js"
    },
    "./esm/GulpMemoryFs": {
      "import": "./esm/GulpMemoryFs.js"
    },
    "./esm/GulpMemoryFs.js": {
      "import": "./esm/GulpMemoryFs.js"
    },
    "./esm/server": {
      "import": "./esm/server.js"
    },
    "./esm/server.js": {
      "import": "./esm/server.js"
    }
  },
  "scripts": {
    "build": "rimraf lib && rimraf esm && NODE_OPTIONS=\"--experimental-json-modules\" gulp",
    "example": "gulp --gulpfile test/gulpfile.mjs",
    "test": "mocha ./test/test.js --timeout 180000",
    "eslint": "eslint ./ --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@bbkkbkk/http-proxy": "^1.17.0",
    "@koa/router": "^10.1.0",
    "chalk": "^4.1.2",
    "http-proxy-middleware": "^1.3.1",
    "internal-ip": "^6.2.0",
    "koa": "^2.13.1",
    "koa-connect": "^2.1.0",
    "memfs": "^3.2.2",
    "mime-types": "^2.1.32",
    "module-alias": "^2.2.2",
    "plugin-error": "^1.0.1",
    "through2": "^4.0.2",
    "ws": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@sweet-milktea/utils": "^3.0.2",
    "@types/koa": "^2",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-node": "^0.3.5",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "glob": "^7.1.7",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-terser": "^2.0.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "mocha": "^9.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs,ts,tsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@3.0.0"
}
